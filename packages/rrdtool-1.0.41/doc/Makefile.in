all-local: link txt man html

.pod.1 .pm.1 .pl.1:
	pod2man --release=$(VERSION) --center=rrdtool $<  > $@

.1.txt:
	@NROFF@ -man -Tlp $< > $@

.1.ps:
	@TROFF@ -man $< > $@

.ps.pdf:
	distill $<

.pm.html .pod.html .pl.html:
	pod2html --infile=$< --outfile=$@ --noindex --htmlroot=. --podpath=. --title=$*

RRDs.pm:
	ln -s ../perl-shared/RRDs.pm .

RRDp.pm:
	ln -s ../perl-piped/RRDp.pm .

